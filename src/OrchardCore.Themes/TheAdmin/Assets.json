[
  {
    "generateSourceMaps": false,
    "generateRTL": false,
    "inputs": [
      "Assets/scss/index.scss"
    ],
    "watch": [
      "Assets/scss/**/*.scss"
    ],
    "output": "wwwroot/css/TheAdmin.css"
  },
  {
    "generateSourceMaps": false,
    "inputs": [
      "Assets/js/*.js"
    ],
    "watch": [
      "Assets/js/*.js"
    ],
    "output": "wwwroot/js/TheAdmin.js"
  },
  {
    "generateSourceMaps": false,
    "generateRTL": false,
    "inputs": [
      "Assets/scss/login.scss"
    ],
    "watch": [
      "Assets/scss/componenets/_validations.scss"
    ],
    "output": "wwwroot/css/login.css"
  },
  {
    "generateSourceMaps": false,
    "inputs": [
      "Assets/js/header/global/*.js",
      "Assets/js/header/*.js"
    ],
    "watch": [
      "Assets/js/header/**/*.js"
    ],
    "output": "wwwroot/js/TheAdmin-header.js"
  }
]
