[
  {
    "generateSourceMaps": false,
    "generateRTL": false,
    "inputs": [
      "Assets/scss/index.scss"
    ],
    "watch": [
      "Assets/scss/**/*.scss"
    ],
    "output": "wwwroot/css/TheAdmin.css"
  },
  {
    "generateSourceMaps": false,
    "inputs": [
      "Assets/js/*.js"
    ],
    "watch": [
      "Assets/js/*.js"
    ],
    "output": "wwwroot/js/TheAdmin.js"
  },
  {
    "generateSourceMaps": false,
    "inputs": [
      "Assets/js/header/*.js"
    ],
    "watch": [
      "Assets/js/header/*.js"
    ],
    "output": "wwwroot/js/TheAdmin-header.js"
  },
  {
    "generateSourceMaps": false,
    "inputs": [
      "node_modules/material-icons/iconfont/material-icons.scss"
    ],
    "output": "wwwroot/fonts/material-icons/material-icons.css"
  },
  {
    "copy": true,
    "inputs": [
      "node_modules/material-icons/iconfont/*.woff",
      "node_modules/material-icons/iconfont/*.woff2"
    ],
    "output": "wwwroot/fonts/material-icons/@"
  }
]
