@model dynamic

<div class="card">
    <div class="card-block">
        @Display(Model.Content)
    </div>
    <div class="card-footer text-muted text-xs-right">
        <a class="btn btn-primary btn-sm thumbnail-link-create" asp-route-action="Create" asp-route-controller="Admin" asp-route-contentitemid="@Model.MenuItemType" asp-route-menuContentItemId="@Model.MenuContentItemId" asp-route-area="Orchard.Menu">@T["Add"]</a>
    </div>
</div>
