@model dynamic

<div class="card">
    <div class="card-body">
        @await DisplayAsync(Model.Content)
    </div>
    <div class="card-footer text-muted text-xs-right">
        <a class="btn btn-primary btn-sm" asp-route-action="Create" asp-route-controller="Activity" asp-route-activityname="@Model.Activity.Name" asp-route-workflowdefinitionid="@Model.WorkflowDefinitionId">@T["Add"]</a>
    </div>
</div>
