{
  "webroot": "wwwroot",
  "version": "2.0.0-*",
  "dependencies": {
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-*",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0-*",
    "Microsoft.NETCore.App": {
      "version": "1.0.0-*",
      "type": "platform"
    },
    "Orchard.Hosting.Console": "2.0.0-*",
    "Orchard.Hosting.Web": "2.0.0-*",
    "Orchard.Core": "2.0.0-*",
    "Orchard.ContentFields": "1.0.0-*"
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "debugType": "portable",
    "preserveCompilationContext": true,
    "copyToOutput": {
      "include": [
        "Views",
        "web.config"
      ]
    },
    "compile": {
      "exclude": [
        "wwwroot",
        "node_modules",
        "App_Data",
        "Core",
        "Modules",
        "Themes"
      ]
    }
  },
  "frameworks": {
    "netstandardapp1.5": {
      "dependencies": {
        "NETStandard.Library": "1.5.0-*"
      },
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ]
    }
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": {
      "version": "1.0.0-*",
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ]
    }
  },
  "packExclude": [
    "node_modules",
    "bower_components",
    "**.kproj",
    "**.user",
    "**.vspscc"
  ],
  "packOptions": {
    "type": "git",
    "url": "https://github.com/OrchardCMS/Orchard2"
  },
  "scripts": {
    "prepublish": [ "gulp publish" ],
    "postrestore": [ "gulp publish" ]
  }
}